head
  meta(charset='UTF-8')
  meta(name='viewport' content='width=device-width, initial-scale=1.0')
  title Massajja Ufarm
  link(rel='stylesheet' href='css/bootstrap/bootstrap.css')
  link(rel='stylesheet' href='css/style.css')
nav.navbar.navbar-expand-lg.navbar-light(style='background-color: #6b8e23')
  a.navbar-brand(href='welcome') Ufarm
  button.navbar-toggler(type='button' data-toggle='collapse' data-target='#navbarSupportedContent' aria-controls='navbarSupportedContent' aria-expanded='false' aria-label='Toggle navigation')
    span.navbar-toggler-icon
  #navbarSupportedContent.collapse.navbar-collapse
    ul.navbar-nav.mr-auto
      li.nav-item.active
        a.nav-link(href='welcome')
          | Home 
          span.sr-only (current)
      li.nav-item
        a.nav-link(href='contactUfarm') Contact Us
      li.nav-item
        a.nav-link(href='aboutUfarm' tabindex='-1' aria-disabled='true') About Us
form.farmerOneRegister(name='urbanFarmerForm' action='/registerUF' method='POST')
  img.formImages(src='img/greenLeafSideImage1.png' alt='')
  h4#registerFO Register here new Urban Farmers
  .form-row
    .form-group.col-md-4
      input#inputFirstName.form-control(type='text' name='firstNameUF' placeholder='First Name')
      span#fnameUFerr
    .form-group.col-md-4
      input#inputLastName.form-control(type='text' name='lastNameUF' placeholder='Last Name')
      span#lnameUFerr
    .form-group.col-md-4
      input#inputUserName.form-control(type='text' name='userNameUF' placeholder='User Name')
      span#unameUFerr
  .form-row
    .form-group.col-md-6
      input#inputDOB.form-control(type='text' name='dobUF' placeholder='Date of Birth')
      span#dobUFerr
    .form-group.col-md-6
      input#inputDOR.form-control(type='text' name='dorUF' placeholder='Date of Registration')
      span#dorUFerr
  .form-row
    .form-group.col-md-6
      input#inputfarmerID.form-control(type='number' name='idNumber' placeholder='Unique farmer ID')
      span#idNumErr
    .form-group.col-md-6
      input#inputfarmerNIN.form-control(type='password' name='ufNIN' placeholder='NIN Number')
      span#ufNINerr
  .form-row
    .form-group.col-md-4
      input#inputPhoneNumber.form-control(type='text' name='phoneNumberUF' placeholder='Phone Contact e.g 0700123456')
      span#ufphoneNumErr1
    .form-group.col-md-4
      input#altPhoneNumber.form-control(type='text' name='phoneNumberUF2' placeholder='Other Contact e.g. 0780123456')
      span#ufphoneNumErr2
    .form-group.col-md-4
      select#inputWard.form-control(name='wardUF')
        option(value='Default' selected='') Ward
        option(value='Busabala') Busabala
        option(value='Massajja') Masajja
        option(value='Kikajjo') Kikajjo
        option(value='Namasuba') Namasuba
  .form-group
    .form-check.form-check-inline
      input#horticultureProduce.form-check-input(type='checkbox' value='Horticulture' name='produceType')
      label.form-check-label(for='inlineCheckbox1') Horticulture Produce
    .form-check.form-check-inline
      input#poultryProducts.form-check-input(type='checkbox' value='Poultry' name='produceType')
      label.form-check-label(for='inlineCheckbox2') Poultry products
    .form-check.form-check-inline
      input#diaryProducts.form-check-input(type='checkbox' value='Diary' name='produceType')
      label.form-check-label(for='inlineCheckbox3') Diary Products
  .form-group
    .form-check-inline
      input#genderM.form-check-input(type='radio' name='gender' value='Male' checked='')
      label.form-check-label(for='genderM')  Male 
    .form-check-inline
      input#genderF.form-check-input(type='radio' name='gender' value='Female')
      label.form-check-label(for='genderF')  Female 
  a(href='loginUF')
    button#registerFO.btn.btn-primary(type='submit' onclick='registerUFform')
      | Register
    br
  //
    <button type="submit" class="btn btn-primary" id="replaceFO">
    Replace FO
    </button>
script(src='./js/massajaUfarm.js')
script(src='js/jquery-3.5.1.slim.min.js')
script(src='js/popper.min.js')
script(src='js/bootstrap/bootstrap.js')
